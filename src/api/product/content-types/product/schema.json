{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "sku": {
      "type": "uid"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "integer"
    },
    "crossedPrice": {
      "type": "integer"
    },
    "stock": {
      "type": "integer"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "video": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "videos"
      ]
    },
    "Documents": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "files"
      ]
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "weight": {
      "type": "decimal"
    },
    "dimensions": {
      "type": "string"
    },
    "warrantyPeriod": {
      "type": "string",
      "default": "1 год"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "mappedBy": "products"
    }
  }
}
